{
  "manifest_version": 3,
  "name": "GSC Bulk Remover",
  "description": "GSC Bulk Remover",
  "version": "1.0",
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png",
    "256": "icon-256.png",
    "512": "icon-512.png",
    "1024": "icon-1024.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}